@model IEnumerable<SP_Share.Models.Item>

@{
    ViewBag.Title = "View";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Content)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Content)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Idx }) |
            @Html.ActionLink("Details", "Details", new { id=item.Idx }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Idx })
        </td>
    </tr>
}

</table>
